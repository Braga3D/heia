<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  metadata-complete="true">
  
  <display-name>Form Validation (A)</display-name>
  <description>JavaScript Lab (SINF2 2015/16)</description>
  
  <servlet>
    <description>Retrieves city name for ZIP code</description>
    <display-name>ZipCodeLookup</display-name>
    <servlet-name>ZipCodeLookup</servlet-name>
    <servlet-class>shop.ZipCodeLookup</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ZipCodeLookup</servlet-name>
    <url-pattern>/ZipCodeLookup</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
      <welcome-file>index.html</welcome-file>
      <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

</web-app>
